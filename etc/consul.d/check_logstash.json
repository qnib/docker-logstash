{
  "services": [
    {
        "id": "ls-syslog",
        "name": "ls-syslog",
        "port": 5514,
        "checks": [{
            "script": "nmap 127.0.0.1 -PN -p 5514 | grep open",
            "interval": "10s"
        }]
    },
    {
        "id": "ls-udp",
        "name": "ls-udp",
        "port": 55514,
        "checks": [{
            "script": "nmap 127.0.0.1 -sU -PN -p 55514 | grep open",
            "interval": "10s"
        }]
    },
    {
        "id": "logstash",
        "name": "logstash",
        "checks": [{
            "script": "supervisorctl status logstash | grep RUNNING",
            "interval": "10s"
        }]
    }
  ]
}
